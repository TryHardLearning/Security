  {
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
      "rewrites": [
        {
          "source": "/",
          "destination": "/index.html",
          "function ": "app"
          
        },
        {
          "source": "/logar",
          "function ": "app"
        }
      ]
    },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ]
}
